import numpy 
import matplotlib.pyplot as plt 
def f(x):
    k=2*x
    n= numpy.sin(k)
    return n**2

assert numpy.isclose(f(0.5),0.7080734182735712)

def df_analytic(x):
    r=4*numpy.sin(2*x)*numpy.cos(2*x)
    return r 

def central_difference(f, x, dx):
    m=(f(x+dx)-f(x-dx))/(2*dx)
    return m 

